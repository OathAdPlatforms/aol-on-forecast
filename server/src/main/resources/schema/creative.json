{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "A creative",
  "type": "object",
  "properties": {
    "ad_format_id": { "type": "integer", "description": "Ad Format ID", "title": "Ad Format ID", "default": 0},
    "ad_title": { "type": "string", "description": "Ad Title", "title": "Ad Title" },
    "adaptv_cdn_size_in_kb": { "type": "integer", "description": "Adap.tv CDN Size In KB", "title": "Adap.tv CDN Size In KB", "required": "true", "default": 0},
    "advertiser_id":  { "type": "number", "description": "Advertiser ID", "title": "Advertiser ID"},
    "brand_id":  { "type": "number", "description": "Brand ID", "title": "Brand ID"},
    "campaign_id": { "type": "integer", "description": "Campaign ID", "title": "Campaign ID"},
    "click_through_url": { "type": "string", "description": "Click Through URL", "title": "Click Through URL"},
    "companion_height": { "type": "integer", "description": "Companion Height", "title": "Companion Height", "default": -2},
    "companion_width": { "type": "integer", "description": "Companion Width", "title": "Companion Width", "default": -2},
    "created_at": { "type": "string", "format": "date-time", "description": "Created At", "title": "Created At", "required": "true"},
    "creative_plugin_properties": { "type": "string", "description": "Creative Plugin Properties", "title": "Creative Plugin Properties"},
    "description": { "type": "string", "description": "Description", "title": "Description"},
    "duration": { "type": "integer", "description": "Duration", "title": "Duration", "default": 0},
    "external_id": { "type": "string", "description": "External ID", "title": "External ID", "required": "true", "default": ""},
    "first_submission_date": { "type": "string", "format": "date-time", "description": "First Submission Date", "title": "First Submission Date"},
    "height": { "type": "integer", "description": "Height", "title": "Height", "default": 0},
    "id": { "type": "integer", "description": "ID", "title": "ID", "required": "true"},
    "is_third_party_served": { "type": "integer", "description": "Is Third Party Served", "title": "Is Third Party Served", "required": "true", "default": 0},
    "is_tps_skippable": { "type": "integer", "description": "Is TPS Skippable", "title": "Is TPS Skippable", "required": "true", "default": 0},
    "is_valid_plugin_properties": { "type": "integer", "description": "Is Valid Plugin Properties", "title": "Is Valid Plugin Properties", "required": "true", "default": 0},
    "name": { "type": "string", "description": "Name", "title": "Name", "required": "true", "default": ""},
    "organization_id": { "type": "integer", "description": "Organization ID", "title": "Organization ID", "required": "true", "default": 0},
    "reviewed_by_ad_ops": { "type": "integer", "description": "Reviewed By Ad Ops", "title": "Reviewed By Ad Ops", "default": 0},
    "reviewed_on_time": { "type": "integer", "description": "Reviewed On Time", "title": "Reviewed On Time", "default": 0},
    "reviewer": { "type": "string", "description": "Reviewer", "title": "Reviewer"},
    "skippable": {"type": "string", "description": "Skippable", "title": "Skippable", "enum": ["ONLY_WHEN_MANDATORY", "WHENEVER_POSSIBLE", "NEVER"], "required": "true", "default": "NEVER"},
    "status": { "type": "integer", "description": "Status", "title": "Status", "required": "true", "default": 0},
    "survey_label": { "type": "string", "description": "Survey Label", "title": "Survey Label"},
    "survey_url": { "type": "string", "description": "Survey URL", "title": "Survey URL"},
    "template_type": { "type": "integer", "description": "Template Type", "title": "Template Type"},
    "updated_at": { "type": "string", "format": "date-time", "description": "Updated At", "title": "Updated At", "required": "true"},
    "vertical_id": { "type": "integer", "description": "Vertical ID", "title": "Vertical ID"},
    "width": { "type": "integer", "description": "Width", "title": "Width", "default": 0}
  }
}

